[user]
  name = Daniele Palombo
  email = ksemuldie@gmail.com
[push]
  default = simple
[core]
  editor = vim
[color]
  diff = auto
  status = auto
  branch = auto
[format]
  pretty = %Cblue%h%Creset %Cgreen[%ar]%Creset (%an) %s
[alias]
  i = init
  c = commit -m
  a = add
  s = status
  d = diff --color-words
  pl = pull
  ps = push
  br = branch
  lg = "!/bin/zsh -c 'source ~/.githelpers && pretty_git_log'"
  l = !git lg -30
  la = !git lg --all
  ll = log --stat --abbrev-commit
  alias = !git config --list | grep 'alias\\.' | sed 's/alias\\.\\([^=]*\\)=\\(.*\\)/\\1\\\t => \\2/' | sort
[push]
  default = current
[pull]
  rebase = true
[github]
  user = DanielePalombo
[hub]
  protocol = https
